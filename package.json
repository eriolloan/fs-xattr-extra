{
  "name": "fs-xattr-extra",
  "type": "module",
  "version": "1.0.1",
  "description": "extra tools for fs-xattr",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/**/*"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node --experimental-specifier-resolution=node ./dist/index.js",
    "test": "jest --passWithNoTests --verbose --coverage"
  },
  "keywords": [],
  "author": "Adrien Facelina",
  "license": "ISC",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "fs-xattr": "^0.4.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  }
}